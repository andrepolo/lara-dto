{
    "name": "andrepolo/lara-dto",
    "description": "Add data transfer objects to your Laravel app to transfer structured data",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Thiel",
            "email": "mail@andrepolo.de"
        }
    ],
    "require": {
        "illuminate/support": "^9.0",
        "kamermans/docblock-reflection": "~1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "AndrePolo\\LaraDto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndrePolo\\LaraDto\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndrePolo\\LaraDto\\DataTransferServiceProvider"
            ]
        }
    }
}
