{
    "name": "andrepolo/laravel-datatransfer",
    "description": "Add data transfer objects to your Laravel app to transfer structured data",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Thiel",
            "email": "mail@andrepolo.de"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.4",
        "kamermans/docblock-reflection": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "AndrePolo\\DataTransfer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndrePolo\\DataTransfer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndrePolo\\DataTransfer\\DataTransferServiceProvider"
            ]
        }
    }
}
